    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "nodemon": "^2.0.20",
    "vitest": "^1.2.2",
    "vitest-tsconfig-paths": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "^5.4.2",
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
